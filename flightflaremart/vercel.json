{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "cleanUrls": true,
  "version": 2,
  "installCommand": "composer install --no-dev --optimize-autoloader && npm install",
  "buildCommand": "npm run build",
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "false",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie"
  }
}
