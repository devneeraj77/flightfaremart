{
  "version": 2,
  "builds": [
    { "src": "api/index.php", "use": "vercel-php@0.7.1" },
    { "src": "public/**", "use": "@vercel/static" }
  ],
  "routes": [
    {
        "src": "/(css|js|img|build|favicon.ico|robots.txt|fonts|svg|webfonts)(.*)",
        "dest": "public/$1$2"
    },
    {
      "src": "/(.*)",
      "dest": "api/index.php"
    }
  ],
  "headers": [
    {
      "source": "/build/assets/.+",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(css|js|img|images|favicon.ico|robots.txt|fonts|svg|webfonts)/.+",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=604800"
        }
      ]
    }
  ],
  "env": {
    "APP_ENV": "production",
    "APP_DEBUG": "true",
    "APP_URL": "https://flightflaremart.vercel.app",
    "APP_CONFIG_CACHE": "/tmp/config.php",
    "APP_EVENTS_CACHE": "/tmp/events.php",
    "APP_PACKAGES_CACHE": "/tmp/packages.php",
    "APP_ROUTES_CACHE": "/tmp/routes.php",
    "APP_SERVICES_CACHE": "/tmp/services.php",
    "VIEW_COMPILED_PATH": "/tmp",
    "CACHE_DRIVER": "array",
    "LOG_CHANNEL": "stderr",
    "SESSION_DRIVER": "cookie"
  }
}